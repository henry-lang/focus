{
    "name": "Focus",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/home.html"
    },
    "permissions": [
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "icons": { "16": "popup/assets/icon-white.png",
        "48": "popup/assets/icon-white.png",
       "128": "popup/assets/icon-white.png" 
    }
}