{
    "name": "Focus",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup/index.html"
    },
    "permissions": [
        "alarms",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}